    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.0",
    "postcss": "latest",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.42.0"
